{
  "entrypoint": "index.html",
  "shell": "src/my-app.js",
  "extraDependencies": [
    "icons/*",
    "style.css",
    ".nojekyll",
    "./node_modules/three/build/three.min.js",
    "./node_modules/three/examples/js/controls/OrbitControls.js",
    "./node_modules/three/examples/js/objects/Sky.js",
    "./node_modules/three/examples/js/exporters/GLTFExporter.js",
    "./node_modules/three/examples/js/loaders/GLTFLoader.js",
    "./node_modules/three/examples/js/utils/BufferGeometryUtils.js",
    "./node_modules/three/examples/js/controls/TransformControls.js"
  ],
  "moduleResolution": "node",
  "npm": true,
  "builds": [
    {
      "name": "default",
      "bundle": false,
      "js": { "minify": true },
      "css": { "minify": true },
      "html": { "minify": true },
      "addServiceWorker": true,
      "addPushManifest": false
    }
  ]
}
